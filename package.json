{
  "name": "lee-storybook",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "A personal storybook full of my components",
  "main": "src/export.tsx",
  "author": "Edward Lee",
  "repository": {
    "type": "git",
    "url": "https://github.com/leedjr-97/lee-storybook"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
